class Car {
    String model;
    int year;

    void display() {
        System.out.println("Model: " + model + ", Year: " + year);
    }
}

public class Main {
    public static void main(String[] args) {
        Car c = new Car();
        c.model = "Toyota";
        c.year = 2022;
        c.display();
    }
}